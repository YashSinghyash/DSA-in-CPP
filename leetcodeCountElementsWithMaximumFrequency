#include <iostream>
#include <unordered_map>
#include <vector>
#include <climits>
using namespace std;


class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        unordered_map <int , int > m;

        int maxFreq = INT_MIN;

        for (int i = 0 ; i < nums.size() ;i ++){
            m[nums[i]]++;
            maxFreq = max(m[nums[i]] , maxFreq);
        }
        int ans = 0;

        for (auto it: m){

            if (it.second == maxFreq){
                ans+=it.second;
            }
        }

        return ans;

    }
};